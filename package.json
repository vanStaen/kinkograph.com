{
  "name": "kinkograph.com",
  "version": "1.0.0",
  "description": "Kinky Photo-templates",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start-client": "react-scripts start",
    "devfr": "cd frontend/; npm i; npm start; cd ../;",
    "test": "cd frontend/; npm run test; cd ../;",
    "dev": "forever -c \"nodemon --exitcrash\" index.js",
    "log": "heroku logs --tail",
    "version": "heroku --version; git --version",
    "buildfe": "cd frontend/; npm run build; cd ../; git add -A; git commit -a -m \"deploy frontend Build\"; git push;",
    "deploy": "npm i; npm run buildfe; heroku git:remote -a kinkograph; git push heroku main",
    "kill": "killall -9 node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanStaen/kinkograph.com.git"
  },
  "keywords": [
    "kinkograph"
  ],
  "dependencies": {
    "aws-sdk": "^2.908.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "jimp": "^0.16.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "path": "^0.12.7",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "forever": "^3.0.4",
    "lint": "^0.7.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1"
  },
  "author": "cvanstaen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanStaen/kinkograph.com/issues"
  },
  "homepage": "https://github.com/vanStaen/kinkograph.com#readme"
}
